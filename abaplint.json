{
  "global": {
    "files": "/src/**/*.*",
    "skipGeneratedGatewayClasses": true,
    "skipGeneratedPersistentClasses": true,
    "skipGeneratedFunctionGroups": true
  },
  "dependencies": [
    {
      "url": "https://github.com/abaplint/deps",
      "folder": "/deps",
      "files": "/src/**/*.*"
    }
  ],
  "syntax": {
    "version": "v750",
    "errorNamespace": "^(Z|Y)",
    "globalConstants": [],
    "globalMacros": []
  },
  "rules": {
    "7bit_ascii": {
      "exclude": []
    },
    "class_attribute_names": {
      "exclude": [],
      "ignoreExceptions": true,
      "statics": "^G._.*$",
      "instance": "^M._.*$"
    },
    "cloud_types": {
      "exclude": []
    },
    "colon_missing_space": {
      "exclude": []
    },
    "commented_code": false,
    "contains_tab": {
      "exclude": []
    },
    "definitions_top": {
      "exclude": []
    },
    "description_empty": {
      "exclude": []
    },
    "empty_statement": {
      "exclude": []
    },
    "exit_or_check": {
      "exclude": []
    },
    "exporting": {
      "exclude": []
    },
    "functional_writing": false,
    "global_class": {
      "exclude": []
    },
    "identical_form_names": {
      "exclude": []
    },
    "indentation": {
      "exclude": [],
      "ignoreExceptions": true,
      "alignTryCatch": false,
      "globalClassSkipFirst": false,
      "ignoreGlobalClassDefinition": false,
      "ignoreGlobalInterface": false
    },
    "inline_data_old_versions": {
      "exclude": []
    },
    "keyword_case": {
      "style": "upper",
      "ignoreFunctionModuleName": true,
      "exclude": ["src/zcl_abapgit_res_repo_files.clas.abap"],
      "ignoreExceptions": true,
      "ignoreLowerClassImplmentationStatement": true,
      "ignoreGlobalClassDefinition": false,
      "ignoreGlobalInterface": false
    },
    "line_length": {
      "exclude": [],
      "length": 120
    },
    "line_only_punc": {
      "exclude": [],
      "ignoreExceptions": true
    },
    "local_class_naming": {
      "exception": "",
      "exclude": [],
      "local": "^LCL_.*$",
      "test": "^LTCL_.*$"
    },
    "local_variable_names": {
      "exclude": [],
      "expectedData": "^L._.*$",
      "expectedConstant": "^LC_.*$",
      "expectedFS": "^<L._.*>$"
    },
    "max_one_statement": {
      "exclude": []
    },
    "message_exists": true,
    "method_length": {
      "exclude": [],
      "errorWhenEmpty": false,
      "ignoreTestClasses": false,
      "statements": 100
    },
    "method_parameter_names": {
      "exclude": [],
      "ignoreExceptions": true,
      "importing": "^I._.*$",
      "returning": "^R._.*$",
      "changing": "^C._.*$",
      "exporting": "^E._.*$",
      "ignoreNames": ["P_TASK"]
    },
    "mix_returning": {
      "exclude": []
    },
    "msag_consistency": {
      "exclude": []
    },
    "nesting": {
      "exclude": ["src/zcl_abapgit_res_repo_push.clas.abap"],
      "depth": 5
    },
    "object_naming": {
      "exclude": [],
      "clas": "^ZC(L|X)\\_",
      "intf": "^ZIF\\_",
      "prog": "^Z",
      "fugr": "^Z",
      "tabl": "^Z",
      "ttyp": "^Z",
      "dtel": "^Z",
      "doma": "^Z",
      "msag": "^Z",
      "tran": "^Z",
      "enqu": "^Z",
      "auth": "^Z",
      "pinf": "^Z",
      "idoc": "^Z",
      "ssfo": "^Z",
      "ssst": "^Z",
      "xslt": "^Z"
    },
    "obsolete_statement": {
      "setExtended": true,
      "occurs": true,
      "exclude": [],
      "refresh": true,
      "compute": true,
      "add": true,
      "subtract": true,
      "multiply": true,
      "move": true,
      "divide": true,
      "requested": true
    },
    "parser_error": {
      "exclude": []
    },
    "sequential_blank": {
      "exclude": [],
      "lines": 4
    },
    "space_before_colon": {
      "exclude": []
    },
    "start_at_tab": {
      "exclude": []
    },
    "superclass_final": false,
    "when_others_last": {
      "exclude": []
    },
    "whitespace_end": {
      "exclude": []
    }
  }
}
